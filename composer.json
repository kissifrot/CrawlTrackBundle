{
  "name": "webdl/crawltrack-bundle",
  "description": "This bundle is used to track crawlers and robots in a Symfony app. It is based on Crawltrack",
  "keywords": [
    "crawltrack", "symfony", "symfony-bundle"
  ],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Philippe Villiers",
      "email": "kissifrot@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "symfony/framework-bundle": "4.*",
    "doctrine/doctrine-bundle": "*",
    "doctrine/orm": "*"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "4.*"
  },
  "suggest": {
    "friendsofsymfony/user-bundle": "Allows users management"
  },
  "autoload": {
    "psr-4": {
      "WebDL\\CrawltrackBundle\\": "src/"
    }
  }
}
